Program lock;

uses wincrt, windos;
{$I c:\filer.lok}

var key : char;
eat : string;
nam : string;

begin
      clrscr;
      writeln( '                      Lock 3.0 copyright Jason Chalom 2006' );
      writeln( 'name:' );
      read(nam);




begin    {main program}
      repeat
      pass;
      read(eat);


if (eat = 'JayLog' ) then
      writeln( 'Access Accepted: ',nam )
         Else
         begin
       writeln( 'Access Denied' );
       clrscr;
       readln;
end;

if (eat = 'forgot' ) then
   by;
   readln;


until (eat = 'JayLog' ) or (eat = 'forgot' );
clrscr;
end;
end.