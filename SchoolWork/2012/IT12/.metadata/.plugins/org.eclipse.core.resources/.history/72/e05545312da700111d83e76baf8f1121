package myGameScore;

import javax.swing.JOptionPane;

public class Test {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		String name = "Jason";
		String vowels = "AIOUEaioue";
		int fconst = name.indexOf(vowels);
		JOptionPane.showMessageDialog(null, "Name: "+name+" first vowel is at: "+fconst);
	}

}
