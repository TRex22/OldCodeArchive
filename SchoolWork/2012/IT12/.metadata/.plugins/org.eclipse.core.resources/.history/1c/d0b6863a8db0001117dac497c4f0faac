public class Clothes {
	
	private String colour;
	private String descr;
	private int rating;
	
	public Clothes (String c, String d, int r)
	{
		setColour(c);
		descr = d;
		rating = r;
	}

	public String getColour() {
		return colour;
	}

	public void setColour(String colour) {
		this.colour = colour;
	}
	
	
	
	
	
}
