package Ques3;

public class Book {
	private int bookNo;
	private String author;
	private String title;
	private String media;
	private String library;

	public Book (int No, String a, String t, String m, String l)
	{
		bookNo = No;
		author = a;
		setTitle(t);
		media = m;
		library = l;
	}

	//public void setTitle(String title) {
	//	this.title = title;
	//}

	public String getTitle() {
		return title;
	}





}
