package myDrawTraingle;

import javax.swing.JOptionPane;

public class DrawTriagngle {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		String output = "";
		for (int loop1=1;loop1<=5;loop1++)
		{
			output+="*";
		}
		
		JOptionPane.showMessageDialog(null, output);
		
		
		
		

	}

}
